{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [ "*" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
    "AllowedHeaders": [ "*" ]
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5050"
      },
      "Https": {
        "Url": "https://*:5051"
      }
    }
  }
}
